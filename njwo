int main() {
	int a = 5;



	std::cin >> a;
	int* urr = new int(a);
	int* arr = new int(a);


		for (int i = 0; i < a; i++) {
			std::cin >> arr[i];



			for (int i = 0; i < a; i++) {
				std::cin >> urr[i];


				int sum(int a, int b);
		#include <iostream>
		for (int i = 0; i < a; i++) {
					int sum = arr[i] + urr[i];
					std::cout << sum << std::endl;


					return 0;
				}
		}

}
